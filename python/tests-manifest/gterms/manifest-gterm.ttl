@prefix : <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix test:    <https://w3c.github.io/N3/tests/test.n3#> .
@prefix mf:      <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix qt:      <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdft:    <http://www.w3.org/ns/rdftest#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<>  rdf:type mf:Manifest ;
    rdfs:label "graph term test cases" ;
    mf:entries ( :graph1 ) .

:graph1
    a   test:TestN3Query ;
    a   test:TestN3Positive ;
    mf:name "graph1" ;
    mf:action [ 
        qt:query <graph1-query.n3> ;
        test:rules <graph1.n3> ;
        qt:data <graph1-data.n3>
    ] ;
    mf:result [
        qt:data <graph1-ref.n3>
    ] ;
    rdfs:comment "..." ;
    rdft:approval rdft:Approved ;
    test:options [ test:conclusions "true"^^xsd:boolean ] .