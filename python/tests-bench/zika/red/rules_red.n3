# utilities

{ ?p ut:has ?r } <= {
    ?p fh:id ?id .
    ?r fh:subject ?id .
} .

# { ?r ut:hasCodeValue ?c } <= { 
#     ?r fh:coding ?cod . 
#     ?cod fh:code ?c 
# } .

{ ?p ut:hasCondition ?c } <= {
    ?p ut:has ?c .
    ?c a fh:Condition .
} .